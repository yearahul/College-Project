 #include <iostream>
#include<string>
using namespace std;
int stringLength(const char* str)
{
    int a=0;
   //cout<<str;
   while(str[a]){
       a++;
   }
    return a;
}
int main(){   //DO NOT change the 'main' signature

    //Fill code here
    string s;
    cin>>s;
   const  char * p = s.c_str();
    int a = stringLength(p);
    cout<<a;
    return 0;
}
