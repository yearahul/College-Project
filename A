 #include <iostream>
using namespace std;


int main()   //DO NOT change the 'main' signature
{
   int num, temp=0, last, rem, pro=0;
   cout<<"Enter a Number: ";
   cin>>num;
   while(num!=0)
   {
      if(temp==0)
      {
         last = num%10;
         temp++;
      }
      rem = num%10;
      num = num/10;
   }
   pro = rem * last;
   cout<<pro;
   return 0;
}

 Multiply first


 #include <iostream>
using namespace std;

class Shop {

    private:
      //Declare the attributes
        string itemName;

        string itemQuantity;

        double itemPrice;
      

    public:
         

    void setItemName(string iname){
        itemName=iname;
    }

    void setItemQuantity(string iqty){
        itemQuantity=iqty;
    }

    void setItemPrice(double iprice){
        itemPrice=iprice;
    }
    string getItemName(){
        return itemName;
    }

    string getItemQuantity(){
        return itemQuantity;
        
    }

    double getItemPrice(){
        return itemPrice;
        
    }
         //Getters and setters for above variables.
   
};

int main(){
        Shop sp;
        string iname;
        double iprice;
        string iqty;
        cout<<"Enter the item name"<<endl;
        cin>>iname;
        cout<<"Enter the item quantity"<<endl;
        cin>>iqty;
        cout<<"Enter the item price"<<endl;
        cin>>iprice;
        sp.setItemName(iname);
        sp.setItemQuantity(iqty);
        sp.setItemPrice(iprice);
        cout<<"Item Name"<<sp.getItemName()<<endl;
        cout<<"Item Quantity"<<sp.getItemQuantity()<<endl;
        cout<<"Item price"<<sp.getItemPrice()<<endl;
        // set the values 
        // get the values and print 
    }


#include <iostream>
#include<string>
using namespace std;
int calculateTicketPrice(int price,int noOfAdults){
    int total=price*noOfAdults;
    return total;
}
 int calculateTicketPrice(int price ,int noOfAdults,int noOfKids){
     int total=(noOfAdults*price)+((price/2)*noOfKids);
     return total;
 }
 int calculateTicketPrice(int price ,int noOfAdults,bool camera){
     int total;
     if(camera==true){
         
          total=(noOfAdults*price)+100;
     }
     else{
          total=noOfAdults*price;
     }
     return total;
 }
 int calculateTicketPrice(int noOfStudents){
     int total=noOfStudents*100;
     return total;
 }	
int main()   //DO NOT change the 'main' signature
{
    //Fill code here
    calculateTicketPrice(1000,3);
    calculateTicketPrice(1000,3,2);
    calculateTicketPrice(10);
   
    return 0;
    
}
